    <macro name="prod_arg_agricultural_macro" class="production">
        <component ref="prod_arg_foodrations" />
        <properties>
            <identification name="{7345565,1}" shortname="{7345565,2}" makerrace="argon" description="{7345565,3}" />
            <build>
                <sets>
                    <set ref="factory" />
                    <set ref="headquarters_player" />
                </sets>
            </build>
            <explosiondamage value="10000" />
            <hull max="133000" />
            <secrecy level="2" />
            <production wares="foodrations meat wheat majasnails sojabeans sojahusk nostropoil spaceweed sunriseflowers swampplant">
                <queue>
                    <item ware="foodrations" />
                    <item ware="meat" />
                    <item ware="wheat" />
                    <item ware="majasnails" />
                    <item ware="sojabeans" />
                    <item ware="sojahusk" />
                    <item ware="nostropoil" />
                    <item ware="spaceweed" />
                    <item ware="sunriseflowers" />
                    <item ware="swampplant" />
                </queue>
            </production>
            <workforce max="90" />
        </properties>
    </macro>
